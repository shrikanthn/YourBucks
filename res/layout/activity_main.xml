<ScrollView 
    xmlns:tools="http://schemas.android.com/tools"
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="524dp"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <AutoCompleteTextView
        android:id="@+id/autoCompleteStockSymbol"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView1"
        android:layout_toLeftOf="@+id/button1"
        android:ems="10" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/autoCompleteStockSymbol"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/textView1"
        android:text="Search" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Stock Search" />

    <TextView
        android:id="@+id/StockHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/autoCompleteStockSymbol"
        android:layout_centerHorizontal="true"
        android:text="Header"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/StockValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/StockHeader"
        android:layout_centerHorizontal="true"
        android:text="Medium Text"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/TextView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:layout_toLeftOf="@+id/TextView03"
        android:paddingLeft="15dip"
        android:paddingTop="5dip"
        android:text="Open"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/TextView02"
        android:layout_alignLeft="@+id/TextView01"
        android:layout_alignRight="@+id/TextView01"
        android:gravity="right"
        android:paddingRight="15dip"
        android:paddingTop="5dip"
        android:text="1,111"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView02"
        android:layout_alignRight="@+id/TextView02"
        android:layout_below="@+id/TextView02"
        android:paddingLeft="15dip"
        android:paddingTop="5dip"
        android:text="Bid"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView03"
        android:layout_alignRight="@+id/TextView03"
        android:layout_below="@+id/TextView03"
        android:gravity="right"
        android:paddingTop="5dip"
        android:paddingRight="15dip"
        android:text="1,111"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView06"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView04"
        android:layout_alignRight="@+id/TextView04"
        android:layout_below="@+id/TextView04"
        android:paddingLeft="15dip"
        android:paddingTop="5dip"
        android:text="Ask"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView07"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView06"
        android:layout_alignBottom="@+id/TextView06"
        android:layout_alignLeft="@+id/TextView05"
        android:layout_alignRight="@+id/TextView05"
        android:gravity="right"
        android:paddingRight="15dip"
        android:paddingTop="5dip"
        android:text="1,111"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView09"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView08"
        android:layout_alignBottom="@+id/TextView08"
        android:layout_alignLeft="@+id/TextView07"
        android:layout_alignRight="@+id/TextView07"
        android:gravity="right"
        android:paddingRight="15dip"
        android:paddingTop="5dip"
        android:text="1,111"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView08"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView06"
        android:layout_alignRight="@+id/TextView06"
        android:layout_below="@+id/TextView06"
        android:paddingLeft="15dip"
        android:paddingTop="5dip"
        android:text="1st Yr Target"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView09"
        android:layout_alignRight="@+id/TextView09"
        android:layout_below="@+id/TextView09"
        android:gravity="right"
        android:paddingRight="15dip"
        android:paddingTop="5dip"
        android:text="1,111"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView11"
        android:layout_alignBottom="@+id/TextView11"
        android:layout_alignLeft="@+id/TextView08"
        android:layout_alignRight="@+id/TextView08"
        android:paddingLeft="15dip"
        android:paddingTop="5dip"
        android:text="Day Range"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView10"
        android:layout_alignRight="@+id/TextView10"
        android:layout_below="@+id/TextView10"
        android:paddingLeft="15dip"
        android:paddingTop="5dip"
        android:text="52wk Range"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView12"
        android:layout_alignBottom="@+id/TextView12"
        android:layout_alignLeft="@+id/TextView11"
        android:layout_alignRight="@+id/TextView11"
        android:paddingTop="5dip"
        android:paddingRight="15dip"
        android:text="52 wk rg"
        android:gravity="right"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView12"
        android:layout_alignRight="@+id/TextView12"
        android:layout_below="@+id/TextView12"
        android:paddingLeft="15dip"
        android:paddingTop="5dip"
        android:text="Volume"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/Volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView14"
        android:layout_alignBottom="@+id/TextView14"
        android:layout_alignLeft="@+id/TextView13"
        android:layout_alignRight="@+id/TextView13"
        android:paddingTop="5dip"
        android:gravity="right"
        android:paddingRight="15dip"
        android:text="vol"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView14"
        android:layout_alignRight="@+id/TextView14"
        android:layout_below="@+id/TextView14"
        android:paddingLeft="15dip"
        android:paddingTop="5dip"
        android:text="Avg Vol(3m)"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/AvgVol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView16"
        android:layout_alignBottom="@+id/TextView16"
        android:layout_alignLeft="@+id/Volume"
        android:layout_alignRight="@+id/Volume"
        android:paddingTop="5dip"
        android:gravity="right"
        android:paddingRight="15dip"
        android:text="avg vol"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/MarketCapVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView18"
        android:layout_alignBottom="@+id/TextView18"
        android:layout_alignLeft="@+id/AvgVol"
        android:layout_alignRight="@+id/AvgVol"
        android:paddingTop="5dip"
        android:gravity="right"
        android:paddingRight="15dip"
        android:text="mar"
        android:visibility="invisible"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView16"
        android:layout_alignRight="@+id/TextView16"
        android:layout_below="@+id/TextView16"
        android:paddingLeft="15dip"
        android:paddingTop="5dip"
        android:text="Market Cap"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/TextView03"
        android:layout_alignRight="@+id/button1"
        android:layout_toRightOf="@+id/textView2"
        android:gravity="right"
        android:paddingRight="15dip"
        android:paddingTop="5dip"
        android:text="1,111"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/autoCompleteStockSymbol"
        android:layout_alignRight="@+id/placerHolderText"
        android:layout_below="@+id/StockIndicator"
        android:layout_marginTop="22dp"
        android:paddingLeft="15dip"
        android:paddingTop="5dip"
        android:text="Prev Close"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/stockChartImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView18"
        android:layout_alignRight="@+id/MarketCapVal"
        android:layout_below="@+id/MarketCapVal"
        android:layout_marginTop="24dp"
        android:visibility="invisible"
        android:src="@drawable/down_r" />

    <TextView
        android:id="@+id/placerHolderText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/StockValue"
        android:layout_below="@+id/StockIndicator"
        android:text="Small Te"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:visibility="invisible" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/MarketCapVal"
        android:layout_alignRight="@+id/stockChartImage"
        android:layout_below="@+id/stockChartImage"
        android:layout_marginTop="16dp"
        android:visibility="invisible"
        android:text="Facebook"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <Button
        android:id="@+id/Button01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button2"
        android:layout_alignBottom="@+id/button2"
        android:layout_alignLeft="@+id/stockChartImage"
        android:layout_alignRight="@+id/TextView18"
        android:visibility="invisible"
        android:text="News Headlines"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/StockIndicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/StockValue"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="StockIndicator"
        android:textAlignment="gravity"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textStyle="bold"
        android:visibility="invisible" />

</RelativeLayout>
</ScrollView>